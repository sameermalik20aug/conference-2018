<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shubhamgulati.confluence">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="I C E I L 2018"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/MyTheme2"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2Activity"
            android:launchMode="singleTop"
            android:theme="@style/MyTheme2" />
        <activity
            android:name=".ProfessorActivity"
            android:label="Speakers"
            android:screenOrientation="portrait"
            android:parentActivityName=".Main2Activity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Directions"
            android:label="Directions"
            android:screenOrientation="portrait"
            android:parentActivityName=".Main2Activity"
            />
    </application>

</manifest>